<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#4d4dd9" />
            <gradient android:angle="-90" android:startColor="#3b3ba9" android:endColor="#3448af"  />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#2d62d4" />
            <gradient android:angle="-90" android:startColor="#2678d6" android:endColor="#2777d9" />
        </shape>
    </item>
</selector>